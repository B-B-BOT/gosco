{
    "build": {
      "builder": "python:3.12",
      "phases": {
        "install": {
          "commands": [
            "python -m venv venv",
            "venv\\Scripts\\activate.bat",
            "pip install -r requirements.txt",
            "python manage.py migrate"
          ]
        }
      }
    },
    "start": {
      "command": "venv\\Scripts\\activate.bat && gunicorn bicot.wsgi -b 0.0.0.0:%PORT%"
    }
}